#include <stdlib.h>

#include <time.h>

/* more headers goes there */
#include <stdio.h>


/* betty style doc for function main goes there */

/**
 * main - Entry point function
 * Description - prints combination of three digit numbers
 * Return: 0 (no errors)
 */

int main(void)
{
/* your code goes there */
int i = 0;
int j = 0;
int k = 0;
for (; i < 10; i++)
{
j = i + 1;
for (k = j + 1; k < 10; k++)
{
putchar (i % 10 + '0');
putchar (j % 10 + '0');
putchar (k % 10 + '0');
if (i != 7)
{
putchar (',');
putchar (' ');
}
}
}
putchar ('\n');
return (0);
}
